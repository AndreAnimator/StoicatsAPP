{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    { "src": "/server/index.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/angular" }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/server/index.js" },
    { "source": "(.*)", "destination": "/src/index.html" }
  ]
}
